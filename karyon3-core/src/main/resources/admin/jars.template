<table id="data-table" class="table table-striped table-bordered">
  <thead>
    <th>Name</th>
    <th>Build Date</th>
    <th>Version</th>
  </thead>
  <tbody>
    {{#rows}}
    <tr>
      <td>{{name}}</td>
      <td>{{buildDate}}</td>
      <td>{{implementationVersion}}</td>
    </tr>
    {{/rows}}
  </tbody>
</table>